@page "/{handler?}"
@namespace Blazor.OIDC.Example.Pages

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
	Layout = "_Layout";
}

@using Blazor.OIDC.Components
@model Blazor.OIDC.Pages._HostModel

@if (User.Identity.IsAuthenticated)
{
	<component type="typeof(App)" render-mode="ServerPrerendered" />
}
else
{

	<component type="typeof(LoginWithProviders)" render-mode="ServerPrerendered" />
}